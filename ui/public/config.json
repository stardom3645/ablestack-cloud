{
  "apiBase": "/client/api",
  "servers": [
    {
      "name": "Local-Server",
      "apiHost": "",
      "apiBase": "/client/api"
    }
  ],
  "docBase": "http://docs.ablecloud.io/latest",
  "appTitle": "ABLESTACK-Mold",
  "footer": "â“’ 2021-2025 ABLECLOUD Inc. All Rights Reserved.",
  "buildVersion": "V4.0-4.0.15",
  "loginFooter": "",
  "logo": "assets/logo-ablestack.png",
  "whiteLogo": "assets/white-logo-ablestack.png",
  "miniLogo": "assets/mini-logo-ablestack.png",
  "banner": "assets/login-logo-ablestack.png",
  "loginTitle": "ABLESTACK",
  "loginFavicon": "assets/ablestack-favicon.png",
  "error": {
    "403": "assets/403.png",
    "404": "assets/404.png",
    "500": "assets/500.png"
  },
  "theme": {
    "@layout-mode": "light",
    "@logo-background-color": "#ffffff",
    "@mini-logo-background-color": "#ffffff",
    "@navigation-background-color": "#ffffff",
    "@project-nav-background-color": "#001529",
    "@project-nav-text-color": "rgba(255, 255, 255, 0.65)",
    "@navigation-text-color": "rgba(0, 0, 0, 0.65)",
    "@primary-color": "#1890ff",
    "@link-color": "#1890ff",
    "@link-hover-color": "#40a9ff",
    "@loading-color": "#1890ff",
    "@processing-color": "#1890ff",
    "@success-color": "#52c41a",
    "@warning-color": "#faad14",
    "@error-color": "#f5222d",
    "@font-size-base": "14px",
    "@heading-color": "rgba(0, 0, 0, 0.85)",
    "@text-color": "rgba(0, 0, 0, 0.65)",
    "@text-color-secondary": "rgba(0, 0, 0, 0.45)",
    "@disabled-color": "rgba(0, 0, 0, 0.25)",
    "@border-color-base": "#d9d9d9",
    "@border-radius-base": "4px",
    "@box-shadow-base": "0 2px 8px rgba(0, 0, 0, 0.15)",
    "@logo-width": "240px",
    "@logo-height": "100%",
    "@logo-magin-top": "4px",
    "@logo-magin-bottom": "0px",
    "@mini-logo-magin-top": "8px",
    "@mini-logo-magin-bottom": "8px",
    "@mini-logo-width": "80px",
    "@mini-logo-height": "48px",
    "@banner-width": "500px",
    "@banner-height": "100%",
    "@error-width": "256px",
    "@error-height": "256px"
  },
  "keyboardOptions": {
    "us": "label.standard.us.keyboard",
    "uk": "label.uk.keyboard",
    "fr": "label.french.azerty.keyboard",
    "jp": "label.japanese.keyboard",
    "sc": "label.simplified.chinese.keyboard"
  },
  "userCard": {
    "title": "label.help",
    "icon": "question-circle-outlined",
    "links": [
      {
        "title": "Documentation",
        "text": "ABLESTACK documentation website",
        "link": "http://docs.ablecloud.io/latest",
        "icon": "read-outlined"
      },
      {
        "title": "Email Support",
        "text": "Join ABLESTACK users mailing list to seek and provide support",
        "link": "mailto:support@ablecloud.io",
        "icon": "mail-outlined"
      },
      {
        "title": "Report Issue",
        "text": "Submit a bug or improvement request",
        "link": "https://github.com/ablecloud-team/ablestack-cloud/issues/new",
        "icon": "bug-outlined"
      }
    ]
  },
  "plugins": [],
  "apidocs": true,
  "basicZoneEnabled": true,
  "multipleServer": false,
  "allowSettingTheme": true,
  "docHelpMappings": {}
}
